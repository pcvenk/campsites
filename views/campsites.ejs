<% include layouts/header %>

<h1>Camping Grounds</h1>

<div>
    <% campgrounds.forEach(function(campground){ %>
        <div>
            <h3><%= campground.name %></h3>
            <img src="<%= campground.image %>" width="600" height="450">
        </div>
    <% }) %>
</div>

<% include layouts/footer%>